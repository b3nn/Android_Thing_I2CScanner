<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity" >

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:paddingStart="10dp"
        android:lines="2"
        android:id="@+id/bus_found"
        android:text="@string/bus_found"
        android:fontFamily="Arial"
        android:typeface="serif"
        android:textSize="20sp" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/bus_found"
        android:layout_marginTop="10dp"
        android:paddingStart="10dp"
        android:lines="2"
        android:id="@+id/address_found"
        android:text="@string/address_found"
        android:fontFamily="Arial"
        android:typeface="serif"
        android:textSize="20sp" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/address_found"
        android:layout_marginTop="10dp"
        android:paddingStart="10dp"
        android:lines="2"
        android:id="@+id/address_not_found"
        android:text="@string/address_not_found"
        android:fontFamily="Arial"
        android:typeface="serif"
        android:textSize="20sp" />

</RelativeLayout>

